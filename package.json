    "test": "ava",
    "precoverage": "npm run build",
    "coverage": "c8 ava"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "peggy": "^4.0.2"
  },
  "devDependencies": {
    "@peggyjs/coverage": "1.1.0",
    "@peggyjs/eslint-config": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "ava": "6.1.3",
    "c8": "9.1.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18"
  }
}
